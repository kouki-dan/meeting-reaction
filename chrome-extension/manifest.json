{
    "manifest_version": 2,
    "name": "Meeting Reaction",
    "version": "1.0",
    "description": "Allows you to play sound effects on Google Meet",
    "icons":  {
        "128": "icon.png"
    },
    "content_scripts": [
      {
        "matches": ["https://meet.google.com/*"],
        "js": ["content.js"],
        "run_at": "document_end"
      }
    ],
    "permissions": [
      "https://meet.google.com/"
    ],
    "web_accessible_resources": [
      "script.js",
      "controller.js"
    ]
  }
  